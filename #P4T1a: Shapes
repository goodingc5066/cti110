import turtle
win = turtle.Screen()
t = turtle.Turtle()

#instructions for square to be drawn
t.forward(70)
t.right(90)
t.forward(70)
t.right(90)
t.forward(70)
t.right(90)
t.forward(70)

#instructions for triangle to be drawn
t.pensize(4)
t.pencolor("green")
t.shape("turtle")

        
t.forward(90)
t.left(120)
t.forward(90)
t.left(120)
t.forward(90)




win.mainloop()
